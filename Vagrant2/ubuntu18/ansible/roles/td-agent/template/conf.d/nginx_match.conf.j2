<match nginx>
  type s3
  aws_key_id xxxxxx
  aws_sec_key xxxxx
  s3_bucket xxxxx
  s3_region ap-northeast-1
  s3_object_key_format %{path}%{time_slice}_prd-xxx_web1_%{index}.%{file_extension}
  path nginx-logs/
  buffer_path /var/log/td-agent/s3/nginx-logs/
  time_slice_format %Y/%m/%d
  time_slice_wait 1m
  flush_at_shutdown true
  buffer_chunk_limit 1g
  overwrite false
</match>
